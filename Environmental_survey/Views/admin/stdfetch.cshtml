@model IEnumerable<Environmental_survey.Models.user>

@{
    ViewData["Title"] = "stdfetch";
    Layout = "~/Views/Shared/Dashboard_header_footer.cshtml";
}


<div class="container">
    <table class="table table-striped table-hover table-bordered border-dark">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Group_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Group_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Group_Contact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Group_Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Num_Of_prpoles)
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Group_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Group_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Group_Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Group_Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Num_Of_prpoles)
            </td>
            <td>
                <a asp-action="stdparticipateupdate" class="btn btn-success" asp-route-id="@item.Id">Update</a>
                <a asp-action="stdparticipatedelete" class="btn btn-danger" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>
