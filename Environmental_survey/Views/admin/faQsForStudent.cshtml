@model IEnumerable<Environmental_survey.Models.FaQs>

@{
    ViewData["Title"] = "faQsForStudent";
    Layout = "~/Views/Shared/User.cshtml";
}


<div class="container">
    <div class="row">



@foreach (var item in Model) {
 <div class="col-lg-12">
                <img src="~/dashboard/assets/images/profile/FAQs2.jpg" width="100%" height="200px" />
                <br>
                <br>

     <div class="card">
         <div class="container mt-3">

    <h5>
                        @Html.DisplayFor(modelItem => item.Question)

    </h5>
<p>

                        @Html.DisplayFor(modelItem => item.Answer)

</p>

</div>
                </div>
</div>
}
    </div>

</div>